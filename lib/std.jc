let filter(fn, array)
	| isEmpty(array) = []
	| fn(head(array)) = concat([head(array)], filter(fn, tail(array)))
	| else = filter(fn, tail(array))

let map(fn, array)
	| isEmpty(array) = []
	| else = concat(fn(head(array)), map(tail(array)))

let min(x, y) = x < y ? x : y

let max(x, y) = x > y ? x : y
